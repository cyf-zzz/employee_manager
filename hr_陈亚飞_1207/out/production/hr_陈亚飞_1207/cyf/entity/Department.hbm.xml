<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cyf.entity.Department" table="department" schema="chenyafei1207">
        <id name="id">
            <column name="id" sql-type="int(10)"/>
        </id>
        <property name="dname">
            <column name="dname" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="loc">
            <column name="loc" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>